<%= form_for @bproblem, html: { multipart: true } do |f| %>

  <%= f.text_field :name, placeholder: 'name' %>
  <%= f.select :grade, Bproblem::GRADES %>
  <%= f.text_area :description, placeholder: 'description' %>
  <%= f.collection_select(:sub_area_id, SubArea.all, :id, :name, {:selected => @sub_area_id} ) %>

  <% #photo uploads %>
  <%= f.file_field :photos, multiple: true %>
  <% f.hidden_field :photos_cache %>

<%= f.submit "Submit"%>

<% end %>
